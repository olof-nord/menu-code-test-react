{
  "name": "ot-menu-test",
  "version": "1.0.0",
  "description": "A small scaffold application for building ot-menu coding challenge",
  "private": true,
  "scripts": {
    "start-server": "node src/server/index.js",
    "start-app": "webpack-dev-server --mode development --open",
    "start": "concurrently \"npm:start-server\" \"npm:start-app\"",
    "test-server": "jest --testNamePattern='GraphQL|REST' --config=backend.jest.config.js",
    "test-app": "jest --testNamePattern='App' --config=frontend.jest.config.js",
    "test": "npm run test-server && npm run test-app",
    "build": "webpack --progress",
    "browserslist": "browserslist"
  },
  "author": "OpenTable",
  "license": "ISC",
  "dependencies": {
    "@mantine/core": "^4.2.2",
    "@mantine/hooks": "^4.2.2",
    "@mantine/notifications": "^4.2.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.1",
    "graphql-request": "^4.2.0",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.38.0",
    "react-router-dom": "^6.3.0",
    "regenerator-runtime": "^0.13.9",
    "tabler-icons-react": "^1.47.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "babel-loader": "^8.0.2",
    "browserslist": "^4.20.3",
    "concurrently": "^6.2.1",
    "dotenv-webpack": "^7.1.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^28.0.2",
    "jest-environment-jsdom": "^28.0.2",
    "msw": "^0.39.2",
    "source-map-loader": "^3.0.1",
    "supertest": "^6.2.3",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0"
  }
}
